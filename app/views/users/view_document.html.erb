<p id="notice"><%= notice %></p>

<div class="container">
  <h3>Documents</h3>

  <table class="table table-striped">
    <thead>
    <tr>
      <th>Url</th>
      <th>File name</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @documents.each do |document| %>
      <tr>
        <td><%= document.url %></td>
        <td><%= document.file_name %></td>
        <td><%= document.type %></td>
        </tr>
    <% end %>
    </tbody>
  </table>

  <div class="btn btn-secondary"><%= link_to 'Back', @user, :style => "color: white" %></div>
</div>
